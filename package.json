  "scripts": {
    "lint:es": "eslint -c eslintrc.json 'src/**/*.ts'",
    "lint:ts": "tslint -c tslint.json 'src/**/*.ts'",
    "lint": "npm run lint:es && npm run lint:ts",
    "build": "webpack --config webpack.config.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/react": "^16.9.44",
    "@types/react-native": "^0.63.4",
    "babel-eslint": "^10.1.0",
    "babel-preset-react-native": "^4.0.1",
    "eslint": "^7.6.0",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-native": "^3.8.1",
    "react": ">=16.8.0",
    "react-native": "0.63.2",
    "react-native-typescript-transformer": "^1.2.13",
    "ts-loader": "^8.0.2",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-native": "0.63.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "dependencies": {
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "tslint": "^6.1.3",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^5.0.0",
    "tslint-react-recommended": "^1.0.15"
  }
