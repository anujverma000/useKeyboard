  "description": "React Native hook for keyboard",
  "main": "src/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "react-native",
    "react-native hook",
    "hook",
    "keyboard",
    "useKeyboard"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/anujverma000/useKeyboard.git"
  },
  "bugs": "https://github.com/anujverma000/useKeyboard/issues",
